{
    "name": "formapro/message-queue",
    "type": "library",
    "description": "Message Queue Library",
    "keywords": ["messaging", "queue", "amqp", "rabbitmq"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "symfony/console": "^2.8|^3",
        "symfony/framework-bundle": "^2.8|^3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": { "Formapro\\MessageQueue\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
