{
    "name": "fp/message-queue",
    "type": "library",
    "description": "Message Queue Library",
    "keywords": ["messaging", "queue", "amqp", "rabbitmq"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "symfony/console": "~2.8",
        "symfony/dependency-injection": "~2.8",
        "symfony/config": "~2.8",
        "doctrine/dbal": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": { "FormaPro\\MessageQueue\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
