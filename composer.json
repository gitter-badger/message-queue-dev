{
  "name": "formapro/dev-message-queue",
  "type": "project",
  "require": {
    "php": ">=5.6",
    "formapro/jms": "dev-master",
    "formapro/message-queue": "dev-master",
    "formapro/stomp": "dev-master",
    "formapro/message-queue-bundle": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "doctrine/doctrine-bundle": "~1.2",
    "symfony/monolog-bundle": "^2.8|^3",
    "symfony/browser-kit": "^2.8|^3",
    "symfony/expression-language": "^2.8|^3"
  },
  "config": {
    "bin-dir": "bin"
  },
  "repositories": [
    {
      "type": "path",
