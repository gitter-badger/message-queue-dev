{
    "name": "formapro/job-queue",
    "type": "symfony-bundle",
    "description": "Job Queue",
    "keywords": ["messaging", "queue", "jobs"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:askozienko/MessageQueue.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^2.8|^3",
        "formapro/message-queue": "dev-master",
        "doctrine/orm": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/browser-kit": "^2.8|^3",
        "symfony/expression-language": "^2.8|^3"
    },
    "autoload": {
        "psr-4": { "Formapro\\JobQueue\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
