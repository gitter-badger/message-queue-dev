{
    "name": "formapro/stomp",
    "type": "library",
    "description": "Message Queue Stomp Transport",
    "keywords": ["messaging", "queue", "stomp"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "stomp-php/stomp-php": "^4",
        "formapro/fms": "dev-master",
        "ramsey/uuid": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4.0",
        "formapro/message-queue-test": "dev-master",
        "formapro/message-queue": "dev-master",
        "symfony/framework-bundle": "~2.8",
        "symfony/browser-kit": "~2.8"
    },
    "autoload": {
        "psr-4": { "Formapro\\Stomp\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "suggest": {
        "formapro/message-queue": "If you'd like to use advanced features like Client abstract layer or Symfony integration features"
    },
    "minimum-stability": "dev"
}
