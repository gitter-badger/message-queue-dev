{
    "name": "formapro/message-queue-bundle",
    "type": "symfony-bundle",
    "description": "Message Queue Bundle",
    "keywords": ["messaging", "queue", "amqp", "rabbitmq"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:askozienko/MessageQueue.git"
        }
        ,{
            "type": "vcs",
            "url": "git@gitlab.com:askozienko/MessageQueueDbalTransport.git"
        }
        ,{
            "type": "vcs",
            "url": "git@gitlab.com:askozienko/MessageQueueStompTransport.git"
        }
        ,{
            "type": "vcs",
            "url": "git@gitlab.com:askozienko/MessageQueueJob.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "^2.8|^3",
        "formapro/message-queue": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "formapro/message-queue-stomp-transport": "dev-master",
        "formapro/message-queue-dbal-transport": "dev-master",
        "formapro/message-queue-job": "dev-master",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/monolog-bundle": "^2.8|^3",
        "symfony/browser-kit": "^2.8|^3",
        "symfony/expression-language": "^2.8|^3"
    },
    "suggest": {
        "formapro/message-queue-stomp-transport": "Message queue STOMP transport",
        "formapro/message-queue-dbal-transport": "Message queue dbal transport"
    },
    "autoload": {
        "psr-4": { "Formapro\\MessageQueueBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
