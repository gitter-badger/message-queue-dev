{
    "name": "formapro/amqp-ext",
    "type": "library",
    "description": "Message Queue Amqp Transport",
    "keywords": ["messaging", "queue", "amqp"],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "ext-amqp": "*",
        "formapro/fms": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4.0",
        "formapro/message-queue-test": "dev-master",
        "formapro/message-queue": "dev-master"
    },
    "autoload": {
        "psr-4": { "Formapro\\AmqpExt\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "suggest": {
        "formapro/message-queue": "If you'd like to use advanced features like Client abstract layer or Symfony integration features"
    },
    "minimum-stability": "dev"
}
